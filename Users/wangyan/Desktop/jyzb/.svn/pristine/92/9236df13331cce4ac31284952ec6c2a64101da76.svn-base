import request from '@/utils/request'

export function login(data) {
  return request({
    url: '/login.do',
    method: 'post',
    data
  })
}
export function loginNumber(data) {
  return request({
    url: '/loginNumber.do',
    method: 'post',
    data
  })
}
export function getInfo() {
  return request({
    url: '/getLoginIdByToken.do',
    method: 'post'
  })
}
export function logout(data) {
  return request({
    url: '/logout.do',
    method: 'post',
    data
  })
}