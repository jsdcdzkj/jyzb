import request from '@/utils/request'

// 入库管理导入
export function downTemplate(params) {
  return request({
    url: '/enterWarehouse/toExportTemplate.do',
    method: 'get',
    responseType: "blob",
    params,
  })
}
// 入库管理导入
export function importFile(data) {
  return request({
    url: '/enterWarehouse/toImport.do',
    method: 'post',
    data
  })
}
