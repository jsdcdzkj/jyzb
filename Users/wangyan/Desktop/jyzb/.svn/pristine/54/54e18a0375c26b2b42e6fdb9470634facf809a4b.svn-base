package com.jsdc.rfid.model.warehouse;

import com.baomidou.mybatisplus.annotation.IdType;
import com.baomidou.mybatisplus.annotation.TableField;
import com.baomidou.mybatisplus.annotation.TableId;
import com.baomidou.mybatisplus.annotation.TableName;
import com.baomidou.mybatisplus.extension.activerecord.Model;
import com.fasterxml.jackson.annotation.JsonFormat;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

import javax.persistence.*;
import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

/**
 * 库存详情
 */
@Entity
@TableName("warehousing_stock_detail")
@Table(name = "warehousing_stock_detail")
@Data
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class WarehousingStockDetail extends Model<WarehousingStockDetail> implements Serializable {

    @Id
    @TableId(value = "id", type = IdType.AUTO)
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;

    //库存id
    private Integer  stock_id;

    //使用部门
    private Integer use_dept;

    private Integer enter_id;

    private String enter_no;

    private Integer delivery_id;

    private String delivery_no;

    //仓库id
    private Integer warehouse_id;

    private Integer equip_type;

    @TableField(exist = false)
    @Transient
    private String equip_type_name;

    @TableField(exist = false)
    @Transient
    private String assets_type_code;

    private Integer equip_name;

    @TableField(exist = false)
    @Transient
    private String equip_name_desc;

    private String equip_model;

    private Integer equip_in_num;

    private Integer equip_out_num;

    //计量单位
    private String measurement_unit;

    @TableField(exist = false)
    @Transient
    private Integer equip_num;

    private Integer equip_unit;

    //供应商id
//    private Integer supplier_id;

    //出入库类型 0:入库 1:调拨: 2:借用 3:处置
    private Integer stock_type;

    //所属部门id
    private Integer dept_id;

    //装备状态 0:库存 1:在用
    private Integer equip_status;

    //单价
    private BigDecimal unit_price;

    @TableField(exist = false)
    @Transient
    private BigDecimal price;

    //使用年限
    private Integer use_year;

    //出厂日期
    @JsonFormat(pattern = "yyyy-MM-dd", timezone = "GMT+8")
    private Date produce_date;

    //过期日期
    @JsonFormat(pattern = "yyyy-MM-dd", timezone = "GMT+8")
    private Date expired_date;

    //临期日期
    @JsonFormat(pattern = "yyyy-MM-dd", timezone = "GMT+8")
    private Date critical_date;

    @TableField(exist = false)
    @Transient
    private String produceDate;

    //记账日期
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date account_time;

    @TableField(exist = false)
    @Transient
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date enter_time;

    @TableField(exist = false)
    @Transient
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date delivery_time;

    @TableField(exist = false)
    @Transient
    private Integer homeObject;

    @TableField(exist = false)
    @Transient
    private String homeObjectDesc;

    /**
     * 创建人id
     */
    private Integer create_user;

    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date create_time;

    /**
     * 更新时间
     */
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date update_time;
    /**
     * 更新人id
     */
    private Integer update_user;
    /**
     * 是否删除 0：未删除 1：已删除
     */
    private String is_del;


}
